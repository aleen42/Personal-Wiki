{
    "gitbook": "local",
    "structure": {
        "readme": "README.md"
    },
    "title": "Aleen",
    "styles": {
        "website": "style/website.css"
    },
    "links": {
        "sidebar": {
            "GitHub Project": "https://github.com/aleen42/PersonalWiki"
        }
    },
    "plugins": [
        "github",
        "github-edit",
        "github-search",
        "sharing",
        "ga",
        "advanced-emoji",
        "comments-footer",
        "page-footer",
        "page-treeview",
        "resources",
        "theme-aleen42",
        "math",
        "mind-maps",
        "color"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/aleen42/personalwiki"
        },
        "github-edit": {
            "repo": "aleen42/PersonalWiki",
            "branch": "docs"
        },
        "github-search": {
            "repo": "aleen42/PersonalWiki"
        },
        "sharing": {
            "facebook": true,
            "twitter": false,
            "google": false,
            "weibo": true,
            "instapaper": false,
            "vk": false,
            "all": [
                "facebook",
                "google",
                "twitter",
                "weibo",
                "instapaper",
                "vk"
            ]
        },
        "ga": {
            "token": "G-814XEMT2ZG"
        },
        "injection": {
            "js": [
                "src/index.js"
            ]
        },
        "advanced-emoji": {
            "embedEmojis": true
        },
        "page-footer": {
            "repo": "aleen42/PersonalWiki",
            "issueNum": "20",
            "timeColor": "var(--color-book-theme)",
            "baseUri": "https://wiki.aleen42.com/"
        },
        "page-treeview": {
            "copyright": "Copyright Â© aleen42",
            "style": "markdown",
            "minHeaderCount": "2",
            "minHeaderDeep": "2"
        },
        "resources": {
            "type": "github",
            "repo": "aleen42/PersonalWiki",
            "branch": "docs"
        },
        "comments-footer": {
            "type": "github",
            "repo": "aleen42/PersonalWiki",
            "redirect": "https://wiki.aleen42.com/"
        }
    }
}
